<%= partial "/partials/jumbotron" %> 

<section class="mission">
    <h2>Mission</h2>
    <p><%= data.content.mission %></p>
</section>

<section class="container-fluid content">
  <div class="row">
    <div class="col-md-4 notable-content">
      <a class="hvr-grow" href="<%= config.site_url %>about">
        <h3>Who we are <i class="fa fa-question-circle" aria-hidden="true"></i></h3>
        
      </a>
    </div>
    
    <div class="col-md-4 notable-content">
      <a class="hvr-grow" href="<%= config.site_url %>what_we_do">
        <h3>What we do <i class="fa fa-users" aria-hidden="true"></i></h3>
      </a>
    </div>
    
    <div class="col-md-4 notable-content">
      <a class="hvr-grow" href="<%= config.site_url %>volunteer">
        <h3>Join us <i class="fa fa-smile-o" aria-hidden="true"></i></h3>
      </a>
    </div>
  </div>
</section> 

<aside class="lpevents container-fluid">
  <div class="row">
    <div class="col-md-12">
      <h3>
        Upcoming Events <i class="fa fa-calendar" aria-hidden="true"></i>
      </h3>
      <!-- <ul class="events-listing" id="events-upcoming"></ul> -->
    </div>
  </div>

  <div class="row" id="events-2017">
    <div class="col-md-6">
      <h1>Technology Summer Camp</h1>
      <p><i>Saturdays 10 AM - 4 PM, July 2017</i></p>
      <p>At the Vinson Neighborhood Library</p>
      <p>
        <a href="./register" class="btn btn-lg btn-primary">Sign up!</a>
      </p>
      <div class="embed-responsive embed-responsive-16by9">
        <iframe class="embed-responsive-item" src="//www.google.com/maps/embed/v1/place?q=Vinson%20Neighborhood%20Library,%20Houston,%20Texas&zoom=17&key=AIzaSyBQ8G0XWgVvplGzlNy3BEfxPPsBepnU7zA">
        </iframe>
      </div>
    </div>

    <div class="col-md-6">
      <ul class="events-listing">
        <% data.content.summer_2017_camp.each do |event| %>
          <li>
            <p class="about">
              <span class="summary"><%= event.name %></span>
              <span class="date"><%= event.date %></span>
            </p>
            <p class="description"><%= event.description %></p>
          </li>
        <% end %>
      </ul>
    </div>

  </div>

</aside>
